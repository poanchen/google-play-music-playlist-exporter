{
  "manifest_version": 2,
  "name": "Google Play Music Playlist Exporter",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://play.google.com/music/listen*"
      ],
      "js": [
        "immutable.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://accounts.spotify.com/*",
    "https://api.spotify.com/*"
  ]
}